    package="com.kharismarizqii.tadzkiralqurandoa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TadzkirAlQuranDanDoa">
        <receiver
            android:name=".core.receiver.AlarmReceiver"
            android:enabled="true"
            android:exported="true" />
        <activity android:name=".settings.SettingsActivity"></activity>
        <activity android:name=".alquran.AlquranActivity">
        </activity>
        <activity android:name=".bacaanshalat.BacaanShalatActivity" />
        <activity android:name=".doaharian.DoaHarianActivity" />
        <activity android:name=".asmaulhusna.AsmaulActivity" />
        <activity android:name=".tahlil.TahlilActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>